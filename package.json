    "cypress-xpath": "^2.0.1",
    "esbuild": "^0.14.48",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "multiple-cucumber-html-reporter": "^1.21.4"
